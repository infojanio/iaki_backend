{
  "name": "iaki",
  "version": "1.0.0",
  "description": "IAki style app.",
  "main": "index.js",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "start:dev": "tsx watch src/server.ts",
    "start": "node build/server.js",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "build": "tsup src --out-dir build",
    "postinstall": "prisma generate && prisma migrate deploy",
    "test": "vitest run --dir src/use-cases",
    "test:watch": "vitest --dir src/use-cases",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test:e2e": "vitest run --dir src/http",
    "test:e2e:watch": "vitest --dir src/http",
    "test:coverage": "vitest run --coverage",
    "backup": "mkdir -p backups && docker exec -t iaki_backend-api-iaki-pg-1 pg_dump -U janio -d iaki-marketplace -F c > backups/backup_$(date +%Y%m%d_%H%M%S).dump",
    "restore": "echo 'Informe o arquivo em backups/' && read file && docker exec -i iaki_backend-api-iaki-pg-1 dropdb -U janio --if-exists iaki-marketplace && docker exec -i iaki_backend-api-iaki-pg-1 createdb -U postgres iaki-marketplace && cat backups/$file | docker exec -i iaki_backend-api-iaki-pg-1 pg_restore -U postgres -d iaki-marketplace",
    "test:ui": "vitest --ui"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rocketseat/eslint-config": "2.2.2",
    "@types/bcryptjs": "2.4.6",
    "@types/jsonwebtoken": "9.0.8",
    "@types/node": "^22.10.5",
    "@types/supertest": "6.0.2",
    "@vitest/coverage-v8": "2.1.8",
    "eslint": "8.57.1",
    "npm-run-all": "4.1.5",
    "prisma": "6.1.0",
    "supertest": "7.0.0",
    "ts-node": "10.9.2",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "2.1.8"
  },
  "dependencies": {
    "@fastify/cookie": "11.0.2",
    "@fastify/cors": "10.0.2",
    "@fastify/formbody": "8.0.2",
    "@fastify/jwt": "9.0.3",
    "@prisma/client": "6.1.0",
    "@types/multer": "1.4.12",
    "@types/qrcode": "1.5.5",
    "bcryptjs": "2.4.3",
    "date-fns": "4.1.0",
    "dayjs": "1.11.13",
    "dotenv": "16.4.7",
    "fastify": "^5.2.1",
    "jsonwebtoken": "9.0.2",
    "multer": "1.4.5-lts.2",
    "qrcode": "1.5.4",
    "zod": "3.24.1"
  }
}
